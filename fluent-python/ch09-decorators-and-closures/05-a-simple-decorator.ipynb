   "source": [
    "from clock import clock\n",
    "import time"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "6a20142e-a0e0-4ec6-aa38-b64bcd742cca",
   "metadata": {},
   "source": [
    "## using the decorator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a620a2d7-6f78-413d-9ed1-19c5b7bb8eb2",
   "metadata": {},
   "outputs": [],
   "source": [
    "@clock\n",
    "def snooze(seconds):\n",
    "    time.sleep(seconds)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "8cacc1ef-059c-47fd-ba48-6bbfb0c74601",
   "metadata": {},
   "outputs": [],
   "source": [
    "@clock\n",
    "def factorial(n):\n",
    "    return 1 if n < 2 else n*factorial(n-1)"
   ]
  },
